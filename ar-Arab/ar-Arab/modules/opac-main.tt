<style>
    .topnav {
        overflow: hidden;
        background-color: #03588f;
        display: flex;
        justify-content: center;
        background-image: url("data:image/png;base64,");
        border-radius: 5px;
        z-index: 1000;
        margin: 0px auto;
        line-height: 250%;
        margin-bottom: -23px;
    }

    .topnav a {
        float: right;
        display: block;
        color: #f2f2f2;
        text-align: center;
        padding: 0px 5px;
        text-decoration: none;
        font-size: 17px;
    }

    .topnav a:not(:first-child) {
        background-color: initial;
    }

    .topnav a:hover {
        background-color: #004f7d;
    }

    .topnav a.active {
        background-color: #04AA6D;
        color: white;
    }

    .topnav .icon {
        display: none;
    }

    .topnav a:after {
        /* content: " | "; */
        /* khakdaman */
        /* color: #ffffff; */
    }

    /* @media screen and (max-width: 916px) {
        .item-nav-7 {
            display: none !important;
            background-color: #828586 !important;
        }
    }
    @media screen and (max-width: 1052px) {
        .item-nav-9 {
            display: none !important;
            background-color: #828586 !important;
        }
    } */

    .item-nav-14::after {
        content: "" !important;
    }


    @media screen and (max-width: 742px) {
        /* .item-nav-14 ,.item-nav-11 {
            display: none !important;
            background-color: #828586 !important;
        } */

        .between-nav-item {
            display: none !important;
        }

        .topnav a:not(:first-child) {
            display: none;
            background-color: #00324f;
        }

        .topnav a:not(:first-child):hover {
            background-color: #004f7d;
        }

        .topnav a.icon {
            float: left;
            display: block;
        }

        .topnav a:after {
            content: initial;
        }

        .topnav {
            overflow: hidden;
            background-color: #08090a;
            display: inherit;
            margin-bottom: -16px;
            z-index: 1000;
            position: relative;
            /* height: 49px; */
        }

        .row-nav {
            display: inherit;
        }


        .first-item-nav {
            margin-top: 0px !important;
            padding: 6px 20px !important;
        }

    }

    .main #searchform {
        margin-top: 15px;
    }


    @media screen and (max-width: 1005px) {
        .topnav a {
            font-size: 1.62vw;
        }
    }

    @media screen and (max-width: 742px) {

        .topnav.responsive {
            position: relative;

        }


        .topnav.responsive .icon {
            text-align: left;
            position: absolute;
            left: 0px;
            top: -1px;
        }

        .topnav a {
            margin-bottom: -6px;
            padding: 0 20px;
            font-size: 17px;
        }

        .topnav.responsive a {
            float: none;
            display: block;
            text-align: right;
            margin-top: -1px;
            padding: 2px 20px;
        }
    }
</style>

